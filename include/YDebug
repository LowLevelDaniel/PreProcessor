#include <stdio.h>

#ifndef NL 
  #define NL '\n'
#endif
#ifndef SNL 
  #define SNL "\n"
#endif

#define PRINT_DEBUG(msg, ...) fprintf(stdout, "DEBUG: " msg "\n", ##__VA_ARGS__)
#define PRINT_INFO(msg, ...)  fprintf(stdout, "INFO: " msg "\n", ##__VA_ARGS__)
#define PRINT_WARN(msg, ...)  fprintf(stderr, "WARNING: " msg "\n", ##__VA_ARGS__)
#define PRINT_ERROR(msg, ...) fprintf(stderr, "ERROR: " msg "\n", ##__VA_ARGS__)
